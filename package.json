{
  "name": "popsicle",
  "version": "0.1.0",
  "private": true,
  "main": "./lib",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "web": "webpack-dev-server -d --config ./webpack.config.js  --inline --hot --colors --content-base public/",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "lint": "eslint src",
    "prettier": "prettier --write \"src/**/*.js\"",
    "prettier-eslint": "prettier-eslint --write \"src/**/*.js\""
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|tests))\\.jsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|lottie-react-native|@expo/react-native-action-sheet|popsicle)"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/scripts/**",
      "!**/meta/**",
      "!**/ios/**",
      "!**/app/lib/**",
      "!**/Diagnostics/**"
    ],
    "moduleNameMapper": {
      "^React$": "<rootDir>/node_modules/react"
    }
  },
  "peerDependencies": {
    "expo": "^25.0.0",
    "expo-web": "^0.0.12",
    "jest-snapshots-svg": "^0.0.24",
    "react": "16.2.0",
    "react-dom": "16.0.0",
    "react-native": "0.52.0",
    "react-native-animatable": "^1.2.4",
    "react-native-touchable-native-feedback-safe": "netbeast/react-native-touchable-native-feedback-safe",
    "react-native-web": "^0.4.0",
    "react-navigation": "^1.5.8",
    "webpack": "^3.11.0",
    "webpack-dev-server": "2.9.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-expo-web": "^0.0.5",
    "babel-plugin-react-native-web": "^0.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^3.2.0",
    "file-loader": "^1.1.7",
    "husky": "^0.14.3",
    "jest-expo": "25.0.0",
    "lint-staged": "^7.0.5",
    "prettier-eslint": "^8.7.5",
    "prettier-eslint-cli": "^4.4.0",
    "prettier-miscellaneous": "^1.5.3",
    "react-native-scripts": "next",
    "react-test-renderer": "16.2.0",
    "style-loader": "^0.19.0"
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  }
}
